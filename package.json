{
  "name": "@nuxtjs/apollo",
  "version": "5.0.0-alpha.14",
  "license": "MIT",
  "repository": "https://github.com/nuxt-modules/apollo-module",
  "homepage": "https://apollo.nuxtjs.org",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./config": {
      "types": "./dist/config.d.mts",
      "import": "./dist/config.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue .",
    "release": "standard-version --prerelease alpha && git push --follow-tags && pnpm publish --tag next"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@nuxt/kit": "^4.0.1",
    "@rollup/plugin-graphql": "^2.0.5",
    "@vue/apollo-composable": "^4.2.2",
    "@vue/apollo-option": "^4.2.2",
    "defu": "^6.1.4",
    "destr": "^2.0.5",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.6",
    "jiti": "^1.21.7",
    "ohash": "^1.1.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@nuxt/eslint-config": "^1.7.0",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.7",
    "@nuxt/ui": "^2.22.1",
    "@types/node": "^20.19.9",
    "eslint": "^9.31.0",
    "nuxt": "^3.17.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "@nuxtjs/apollo": "link:."
  },
  "packageManager": "pnpm@10.13.1"
}
